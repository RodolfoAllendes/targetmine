								graphType="BarChart"
								typeClass="org.intermine.model.bio.Gene"  />
-->
      <enrichmentwidgetdisplayer id="compound_enrichment" title="Compound Enrichment"
                                 link="org.intermine.bio.web.widget.CompoundURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.CompoundDataLoader"
                                 label="Compounds"
                                 description="Compounds enriched for items in this list."
                                 filters="DrugBank,ChEMBL,Stitch,PDB"
                                 filterLabel="DataSet" 
                                 externalLink="multi: org.intermine.bio.web.widget.config.CompoundExternalLink" 
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein"
                                 />
 
      <enrichmentwidgetdisplayer id="go_enrichment" title="Gene Ontology Enrichment"
                                 link="org.intermine.bio.web.widget.TmGoStatURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.GoStatLdr"
                                 description="GO terms enriched for items in this list."
                                 label="GO Term"
                                 filters="biological_process,cellular_component,molecular_function"
                                 filterLabel="NS"
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein"
                                 externalLink="http://amigo.geneontology.org/cgi-bin/amigo/term_details?term=" 
                                 />


<!-- chenyian: diable for external server 
-->
      <enrichmentwidgetdisplayer id="omim_enrichment" title="OMIM Enrichment"
                                 link="org.intermine.bio.web.widget.OmimURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.OmimDataLoader"
                                 label="OMIM Disease"
                                 description="Disease enriched for genes in this list."
                                 externalLink="http://www.ncbi.nlm.nih.gov/omim/"
                                 typeClass="org.intermine.model.bio.Gene"
                                 externalLinkLabel="OMIM:" />
      <enrichmentwidgetdisplayer id="protein_omim_enrichment" title="OMIM Enrichment"
                                 link="org.intermine.bio.web.widget.OmimURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.ProteinOmimDataLoader"
                                 label="OMIM Disease"
                                 description="Disease enriched for proteins in this list."
                                 externalLink="http://www.ncbi.nlm.nih.gov/omim/"
                                 typeClass="org.intermine.model.bio.Protein"
                                 externalLinkLabel="OMIM:" />

      <enrichmentwidgetdisplayer id="do_enrichment" title="Disease Ontology Enrichment"
                                 link="org.intermine.bio.web.widget.DoURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.DoDataLoader"
                                 label="DO Term"
                                 description="Disease ontology enriched for items in this list."
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein" 
                                 externalLink="http://django.nubic.northwestern.edu/fundo/databrowse/do_rif/doterm/objects/" 
                                 />

      <enrichmentwidgetdisplayer id="structural_domain_enrichment" title="CATH classification Enrichment"
                                 link="org.intermine.bio.web.widget.StructuralDomainURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.StructuralDomainDataLoader"
                                 label="CATH classification"
                                 description="Structural domains enriched for items in this list."
                                 externalLink="http://www.cathdb.info/cathnode/"
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein" 
                                 externalLinkLabel="CATH:" 
                                 />

</widgets>
</webconfig>
