<webconfig>
  <class className="org.intermine.model.bio.Author">
    <fields>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.BioEntity">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.Chromosome">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="organism.name"/>
      <fieldconfig fieldExpr="length" displayer="/model/sequenceShortDisplayerWithField.jsp" />
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.Comment">
    <fields>
      <fieldconfig fieldExpr="type"/>
      <fieldconfig fieldExpr="text"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.DataSet">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="url" doNotTruncate="true"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.DataSource">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="url" doNotTruncate="true"/>
    </fields>
    <longdisplayers>
      <displayer src="/model/dataSourceLong.jsp"/>
    </longdisplayers>
    <tabledisplayer src="/model/dataSourceShort.jsp"/>
  </class>

  <class className="org.intermine.model.bio.Gene">
    <fields>
      <fieldconfig fieldExpr="ncbiGeneNumber"/>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="secondaryIdentifier"/>
      <fieldconfig fieldExpr="symbol"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="organism.shortName" />
    </fields>
    <longdisplayers>
      <!-- attribute links can now be configured for gene report pages -->
      <displayer src="attributeLinkDisplayer.tile"/>
      <displayer src="cytoscapeNetworkDisplayer.tile"/>
      <!-- displayer src="geneMicroArrayDisplayer.tile"/-->
    </longdisplayers>
    <bagdisplayers>
      <!-- attribute links can now be configured for gene list analysis pages -->
      <displayer src="attributeLinkDisplayer.tile"/>
    </bagdisplayers>
  </class>

 <class className="org.intermine.model.bio.Location">
    <fields>
      <fieldconfig fieldExpr="locatedOn.primaryIdentifier"/>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="strand"/>
    </fields>
  </class>


  <class className="org.intermine.model.bio.SequenceFeature">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="length" displayer="/model/sequenceShortDisplayerWithField.jsp" />
      <fieldconfig fieldExpr="chromosomeLocation" displayer="/model/chromosomeLocDisplayer.jsp" showInResults="false" />
      <fieldconfig fieldExpr="chromosome.primaryIdentifier" showInInlineCollection="false" showInSummary="false" />
      <fieldconfig fieldExpr="chromosomeLocation.start" showInInlineCollection="false" showInSummary="false" />
      <fieldconfig fieldExpr="chromosomeLocation.end" showInInlineCollection="false" showInSummary="false" />
      <fieldconfig fieldExpr="organism.name"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
    <bagdisplayers>
    </bagdisplayers>
  </class>

  <class className="org.intermine.model.bio.OntologyTerm">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.OntologyRelation">
    <fields>
      <fieldconfig fieldExpr="relationship"/>
      <fieldconfig fieldExpr="parentTerm.identifier"/>
      <fieldconfig fieldExpr="childTerm.identifier"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.OntologyTermSynonym">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="type"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Organism">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="taxonId" />
      <fieldconfig fieldExpr="shortName" />
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.Protein">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="primaryAccession"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="organism.name"/>
      <fieldconfig fieldExpr="length" displayer="/model/sequenceShortDisplayerWithField.jsp" />
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
	  <displayer src="/model/proteinAnalysisResult.jsp"/>
	  <displayer src="cytoscapeNetworkDisplayer.tile"/>
    </longdisplayers>
    <bagdisplayers>
    </bagdisplayers>
  </class>

  <class className="org.intermine.model.bio.Publication">
    <fields>
      <fieldconfig fieldExpr="firstAuthor"/>
      <fieldconfig fieldExpr="title"/>
      <fieldconfig fieldExpr="year"/>
      <fieldconfig fieldExpr="journal"/>
      <fieldconfig fieldExpr="volume"/>
      <fieldconfig fieldExpr="pages"/>
      <fieldconfig fieldExpr="pubMedId"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.Sequence">
    <fields>
      <fieldconfig fieldExpr="residues" fieldExporter="org.intermine.bio.web.export.ResidueFieldExporter"
                   sectionOnRight="true" sectionTitle="Residues" openByDefault="true"
                   showInSummary="false" showInResults="false" showInInlineCollection="false"/>
      <fieldconfig fieldExpr="length"/>
    </fields>
    <tabledisplayer src="/model/sequenceShortDisplayer.jsp"/>
  </class>

  <class className="org.intermine.model.bio.Synonym">
    <fields>
      <fieldconfig fieldExpr="value"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinStructure">
    <fields>
      <fieldconfig fieldExpr="pdbId"/>
      <fieldconfig fieldExpr="resolution"/>
      <fieldconfig fieldExpr="experimentType"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.Compound">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.ProteinDomain">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="shortName"/>
      <fieldconfig fieldExpr="type"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.ProteinDomainRegion">
    <fields>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="originalId"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Pathway">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="mainClass" showInSummary="false"/>
      <fieldconfig fieldExpr="subClass" showInSummary="false"/>
    </fields>
    <longdisplayers>
      <displayer src="pathwayLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

<!-- chenyian: disable for external server 
-->
  <class className="org.intermine.model.bio.Disease">
    <fields>
      <fieldconfig fieldExpr="omimId"/>
      <fieldconfig fieldExpr="title"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.DrugProteinInteraction">
    <fields>
      <fieldconfig fieldExpr="protein.primaryAccession"/>
      <fieldconfig fieldExpr="drug.drugBankId" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.Drug">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="drugBankId"/>
      <fieldconfig fieldExpr="genericName" />
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.Enzyme">
    <fields>
      <fieldconfig fieldExpr="ecNumber"/>
      <fieldconfig fieldExpr="description" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinChain">
    <fields>
      <fieldconfig fieldExpr="pdbId" />
      <fieldconfig fieldExpr="chain" />
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.ProteinRegion">
    <fields>
      <fieldconfig fieldExpr="start" />
      <fieldconfig fieldExpr="end" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.ScopEntry">
    <fields>
      <fieldconfig fieldExpr="description" />
      <fieldconfig fieldExpr="sccs" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.ScopClassification">
    <fields>
      <fieldconfig fieldExpr="sid"/>
      <fieldconfig fieldExpr="scopClass.description" />
      <fieldconfig fieldExpr="scopFold.description" />
      <fieldconfig fieldExpr="scopSuperFamily.description" />
      <fieldconfig fieldExpr="scopFamily.description" />
      <fieldconfig fieldExpr="scopDomain.description" />
      <fieldconfig fieldExpr="scopSpecies.description" />
      <fieldconfig fieldExpr="scopProtein.description"  showInSummary="false"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.DOTerm">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true" />
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

  <class className="org.intermine.model.bio.DrugType">
    <fields>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.GeneRIF">
    <fields>
      <fieldconfig fieldExpr="phrase"/>
      <fieldconfig fieldExpr="sentence"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.InteractionExperiment">
    <fields>
      <fieldconfig fieldExpr="description"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.CompoundGroupInteraction">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.CompoundGroup">
    <fields>
      <fieldconfig fieldExpr="inchiKey"/>
    </fields>
    <longdisplayers>
      <displayer src="attributeLinkDisplayer.tile"/>
    </longdisplayers>
  </class>

<!-- exporters -->
  <tableExportConfig id="sequence" className="org.intermine.bio.web.export.SequenceHttpExporter"/>
  <tableExportConfig id="gff3" className="org.intermine.bio.web.export.GFF3HttpExporter"/>
  
<!-- widgets -->
<widgets>
      <enrichmentwidgetdisplayer id="pathway_enrichment" title="Pathway Enrichment"
                                 link="org.intermine.bio.web.widget.TmPathwayURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.PathwayDataLoader"
                                 label="Pathway"
                                 description="Pathways enriched for genes in this list."
                                 filters="All datasets,KEGG,Reactome,NCI" 
                                 filterLabel="DataSet" 
                                 typeClass="org.intermine.model.bio.Gene" />
<!--
      <graphdisplayer id="pathway_distribution" title="Pathway Distribution"
								link="org.intermine.bio.web.widget.PathwayGraphURLGenerator"
								domainLabel="Pathway"
								rangeLabel="Percentage(%)"
								dataSetLoader="org.intermine.bio.web.widget.PathwayDataSetLoader"
								description="Bar chart for pathway enrichment. Only top 10 pathway are shown."
								graphType="BarChart"
								typeClass="org.intermine.model.bio.Gene"  />
-->
      <enrichmentwidgetdisplayer id="protein_pathway_enrichment" title="Pathway Enrichment"
                                 link="org.intermine.bio.web.widget.TmPathwayURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.ProteinPathwayDataLoader"
                                 label="Pathways"
                                 description="Pathways enriched for proteins in this list."
                                 filters="All datasets,KEGG,Reactome,NCI"
                                 filterLabel="DataSet" 
                                 typeClass="org.intermine.model.bio.Protein" />
 
      <enrichmentwidgetdisplayer id="go_enrichment" title="Gene Ontology Enrichment"
                                 link="org.intermine.bio.web.widget.TmGoStatURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.GoStatLdr"
                                 description="GO terms enriched for items in this list."
                                 label="GO Term"
                                 filters="biological_process,cellular_component,molecular_function"
                                 filterLabel="NS"
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein"
                                 externalLink="http://amigo.geneontology.org/cgi-bin/amigo/term-details.cgi?term=" />

<!-- chenyian: diable for external server 
-->
      <enrichmentwidgetdisplayer id="omim_enrichment" title="OMIM Enrichment"
                                 link="org.intermine.bio.web.widget.OmimURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.OmimDataLoader"
                                 label="Disease"
                                 description="Disease enriched for genes in this list."
                                 externalLink="http://www.ncbi.nlm.nih.gov/omim/"
                                 typeClass="org.intermine.model.bio.Gene"
                                 externalLinkLabel="OMIM:" />
<!-- 
      <graphdisplayer id="omim_distribution" title="OMIM disease Distribution"
								domainLabel="OMIM disease"
								rangeLabel="Percentage(%)"
								dataSetLoader="org.intermine.bio.web.widget.OmimDataSetLoader"
								description="Bar chart for OMIM disease enrichment. Only top 10 diseases are shown."
								graphType="BarChart"
								typeClass="org.intermine.model.bio.Gene"  />
-->

      <enrichmentwidgetdisplayer id="protein_omim_enrichment" title="OMIM Enrichment"
                                 link="org.intermine.bio.web.widget.OmimURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.ProteinOmimDataLoader"
                                 label="Disease"
                                 description="Disease enriched for proteins in this list."
                                 externalLink="http://www.ncbi.nlm.nih.gov/omim/"
                                 typeClass="org.intermine.model.bio.Protein"
                                 externalLinkLabel="OMIM:" />

      <enrichmentwidgetdisplayer id="do_enrichment" title="Disease Ontology Enrichment"
                                 link="org.intermine.bio.web.widget.DoURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.DoDataLoader"
                                 label="DO Term"
                                 description="Disease ontology enriched for genes in this list."
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein" 
                                 externalLink="http://django.nubic.northwestern.edu/fundo/databrowse/do_rif/doterm/objects/" />

       <enrichmentwidgetdisplayer id="drug_enrichment" title="Drug Enrichment"
                                 link="org.intermine.bio.web.widget.DrugURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.DrugDataLoader"
                                 label="Drug"
                                 description="Drugs enriched for proteins in this list."
                                 externalLink="http://www.drugbank.ca/drugs/"
                                 typeClass="org.intermine.model.bio.Protein"
                                 externalLinkLabel="DrugBank:" />
 <!--
      <bagtabledisplayer id="table_drugs_protein" title="Associated Drugs"
                         link="org.intermine.bio.web.widget.DrugURLQuery"
                         pathStrings="Protein.drugs.drug"
                         description="Drugs which associated with proteins in the list."
                         displayFields="drugBankId"
                         exportField="primaryAccession"
                         typeClass="org.intermine.model.bio.Protein"
                         columnTitle="Targeted Proteins"/>
-->
      <bagtabledisplayer id="table_drugs_gene" title="Associated Drugs"
                         link="org.intermine.bio.web.widget.DrugURLQuery"
                         pathStrings="Gene.proteins.drugs.drug"
                         description="Drugs which associated with genes in the list."
                         displayFields="drugBankId"
                         exportField="ncbiGeneNumber"
                         typeClass="org.intermine.model.bio.Gene"
                         externalLinkLabel="DrugBank:"
                         externalLink="http://www.drugbank.ca/drugs/"
                         columnTitle="Targeted Genes"/>
<!--
      <bagtabledisplayer id="table_ppis" title="Protein-Protein Interactions"
                         link="org.intermine.bio.web.widget.PpiURLQuery"
                         pathStrings="Protein.proteinInteractions.representativePartner"
                         description="Proteins that interact with proteins in this list."
                         displayFields="primaryAccession"
                         exportField="primaryAccession"
                         typeClass="org.intermine.model.bio.Protein"
                         columnTitle="PPIs"/>
-->

</widgets>
</webconfig>
