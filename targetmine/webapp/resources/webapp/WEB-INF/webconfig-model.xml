<webconfig>
  <class className="org.intermine.model.bio.Author">
    <fields>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.BioEntity">
    <headerconfig>
      <titles>
        <title mainTitles="symbol|primaryIdentifier" numberOfMainTitlesToShow="1" subTitles="*organism.shortName*" />
      </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier" label="DB identifier"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Chromosome">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="organism.name"/>
      <fieldconfig fieldExpr="length" displayer="/model/sequenceShortDisplayerWithField.jsp" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.Comment">
    <fields>
      <fieldconfig fieldExpr="type"/>
      <fieldconfig fieldExpr="text"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.CrossReference">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="source.name"/>
      <fieldconfig fieldExpr="subject.primaryIdentifier"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.DataSet">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="url" doNotTruncate="true"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.DataSource">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="url" doNotTruncate="true"/>
    </fields>
    <tabledisplayer src="/model/dataSourceShort.jsp"/>
  </class>

  <class className="org.intermine.model.bio.Gene">
    <fields>
      <fieldconfig fieldExpr="ncbiGeneNumber"/>
      <fieldconfig fieldExpr="primaryIdentifier" showInSummary="false"/>
      <fieldconfig fieldExpr="secondaryIdentifier" showInSummary="false"/>
      <fieldconfig fieldExpr="symbol"/>
      <fieldconfig fieldExpr="name" showInSummary="false"/>
      <fieldconfig fieldExpr="organism.shortName" />
    </fields>
    <bagdisplayers>
      <!-- attribute links can now be configured for gene list analysis pages -->
      <displayer src="attributeLinkDisplayer.tile"/>
    </bagdisplayers>
  </class>

 <class className="org.intermine.model.bio.Location">
    <fields>
      <fieldconfig fieldExpr="locatedOn.primaryIdentifier"/>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="strand"/>
    </fields>
  </class>


  <class className="org.intermine.model.bio.SequenceFeature">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="length" displayer="/model/sequenceShortDisplayerWithField.jsp" />
      <fieldconfig fieldExpr="chromosomeLocation" displayer="/model/chromosomeLocDisplayer.jsp" showInResults="false" />
      <fieldconfig fieldExpr="chromosome.primaryIdentifier" showInInlineCollection="false" showInSummary="false" />
      <fieldconfig fieldExpr="chromosomeLocation.start" showInInlineCollection="false" showInSummary="false" />
      <fieldconfig fieldExpr="chromosomeLocation.end" showInInlineCollection="false" showInSummary="false" />
      <fieldconfig fieldExpr="organism.name"/>
    </fields>
    <bagdisplayers>
    </bagdisplayers>
  </class>

  <class className="org.intermine.model.bio.Ontology">
    <fields>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.OntologyRelation">
    <fields>
      <fieldconfig fieldExpr="relationship"/>
      <fieldconfig fieldExpr="parentTerm.name"/>
      <fieldconfig fieldExpr="parentTerm.identifier"/>
      <fieldconfig fieldExpr="childTerm.name"/>
      <fieldconfig fieldExpr="childTerm.identifier"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.OntologyTerm">
    <headerconfig>
      <titles>
        <title mainTitles="identifier|name" subTitles="ontology.name" />
      </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.OntologyTermSynonym">
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="type"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Organism">
    <headerconfig>
      <titles>
        <title mainTitles="name"/>
      </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="taxonId" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.Protein">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="primaryAccession"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="organism.name"/>
      <fieldconfig fieldExpr="length" displayer="/model/sequenceShortDisplayerWithField.jsp" />
    </fields>
    <bagdisplayers>
    </bagdisplayers>
  </class>

  <class className="org.intermine.model.bio.Publication">
    <fields>
      <fieldconfig fieldExpr="firstAuthor"/>
      <fieldconfig fieldExpr="title"/>
      <fieldconfig fieldExpr="year"/>
      <fieldconfig fieldExpr="journal"/>
      <fieldconfig fieldExpr="volume"/>
      <fieldconfig fieldExpr="pages"/>
      <fieldconfig fieldExpr="pubMedId"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Sequence">
    <fields>
      <fieldconfig fieldExpr="residues" fieldExporter="org.intermine.bio.web.export.ResidueFieldExporter"
                   sectionOnRight="true" sectionTitle="Residues" openByDefault="true"
                   showInSummary="false" showInResults="false" showInInlineCollection="false"/>
      <fieldconfig fieldExpr="length"/>
    </fields>
    <tabledisplayer src="/model/sequenceShortDisplayer.jsp"/>
  </class>

  <class className="org.intermine.model.bio.Synonym">
    <fields>
      <fieldconfig fieldExpr="value"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinStructure">
    <fields>
      <fieldconfig fieldExpr="pdbId"/>
      <fieldconfig fieldExpr="resolution"/>
      <fieldconfig fieldExpr="experimentType"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.GOAnnotation">
    <fields>
      <fieldconfig fieldExpr="subject.primaryIdentifier"/>
      <fieldconfig fieldExpr="subject.name"/>
      <fieldconfig fieldExpr="subject.symbol"/>
      <fieldconfig fieldExpr="ontologyTerm.identifier" />
      <fieldconfig fieldExpr="ontologyTerm.name" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.GOEvidenceCode">
    <fields>
      <fieldconfig fieldExpr="code"/>
    </fields>
  </class>

    <class className="org.intermine.model.bio.GOTerm">
    <headerconfig>
        <titles>
            <title mainTitles="identifier|name" subTitles="ontology.name" />
        </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="description" showInResults="false" showInInlineCollection="false" doNotTruncate="true" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.Compound">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinDomain">
    <fields>
      <fieldconfig fieldExpr="primaryIdentifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="shortName"/>
      <fieldconfig fieldExpr="type"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinDomainRegion">
    <fields>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="originalId"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinStructureRegion">
    <fields>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="pdbRegion.start"/>
      <fieldconfig fieldExpr="pdbRegion.end"/>
      <fieldconfig fieldExpr="pdbRegion.chain.pdbId"/>
      <fieldconfig fieldExpr="pdbRegion.chain.chain"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.Pathway">
    <headerconfig>
        <titles>
            <title mainTitles="identifier|name" />
        </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="identifier" />
      <fieldconfig fieldExpr="name" />
      <fieldconfig fieldExpr="mainClass" showInSummary="false"/>
      <fieldconfig fieldExpr="subClass" showInSummary="false"/>
    </fields>
  </class>
<!-- chenyian
  <class className="org.intermine.model.bio.GeneSetCluster">
    <fields>
      <fieldconfig fieldExpr="identifier" />
    </fields>
  </class>
-->
  <class className="org.intermine.model.bio.Homologue">
    <fields>
      <fieldconfig fieldExpr="type"/>
      <fieldconfig fieldExpr="gene.ncbiGeneNumber" showInResults="false" showInInlineCollection="false" />
      <fieldconfig fieldExpr="homologue.ncbiGeneNumber"/>
      <fieldconfig fieldExpr="homologue.organism.shortName"/>
    </fields>
  </class>

<!-- chenyian: disable for external server 
-->
  <class className="org.intermine.model.bio.Disease">
    <headerconfig>
        <titles>
            <title mainTitles="omimId|title" />
        </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="omimId"/>
      <fieldconfig fieldExpr="title"/>
    </fields>
  </class>
  <class className="org.intermine.model.bio.ProteinComputationalResult">
    <fields>
      <fieldconfig fieldExpr="computationalAnalysis.algorithm"/>
      <fieldconfig fieldExpr="computationalAnalysis.description"/>
    </fields>
  </class>
  <class className="org.intermine.model.bio.ProteinHitRegion">
    <fields>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="score"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.CompoundProteinInteraction">
    <fields>
      <fieldconfig fieldExpr="protein.primaryAccession"/>
      <fieldconfig fieldExpr="compound.identifier" />
      <fieldconfig fieldExpr="compound.name" />
      <fieldconfig fieldExpr="dataSet.name" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.Enzyme">
    <fields>
      <fieldconfig fieldExpr="ecNumber"/>
      <fieldconfig fieldExpr="description" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinChain">
    <fields>
      <fieldconfig fieldExpr="pdbId" />
      <fieldconfig fieldExpr="chain" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinRegion">
    <fields>
      <fieldconfig fieldExpr="start" />
      <fieldconfig fieldExpr="end" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.StructuralClassification">
    <fields>
      <fieldconfig fieldExpr="type" />
      <fieldconfig fieldExpr="level" />
      <fieldconfig fieldExpr="code" />
      <fieldconfig fieldExpr="description" />
    </fields>
  </class>
  <class className="org.intermine.model.bio.ScopClassification">
    <fields>
      <fieldconfig fieldExpr="level" />
      <fieldconfig fieldExpr="sunid" />
      <fieldconfig fieldExpr="sccs" />
      <fieldconfig fieldExpr="description" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.CathClassification">
    <fields>
      <fieldconfig fieldExpr="level" />
      <fieldconfig fieldExpr="cathCode" />
      <fieldconfig fieldExpr="description" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.StructuralDomain">
    <fields>
      <fieldconfig fieldExpr="identifier" />
      <fieldconfig fieldExpr="structuralClassification.code" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.StructuralRegion">
    <fields>
      <fieldconfig fieldExpr="start" />
      <fieldconfig fieldExpr="startInsertionCode" />
      <fieldconfig fieldExpr="end" />
      <fieldconfig fieldExpr="endInsertionCode" />
      <fieldconfig fieldExpr="proteinChain.identifier" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.DOAnnotation">
    <fields>
      <fieldconfig fieldExpr="subject.primaryIdentifier"/>
      <fieldconfig fieldExpr="subject.name"/>
      <fieldconfig fieldExpr="subject.symbol"/>
      <fieldconfig fieldExpr="ontologyTerm.identifier" />
      <fieldconfig fieldExpr="ontologyTerm.name" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.DOTerm">
    <headerconfig>
        <titles>
            <title mainTitles="identifier|name" subTitles="ontology.name" />
        </titles>
    </headerconfig>
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
      <fieldconfig fieldExpr="description" doNotTruncate="true" />
    </fields>
  </class>

  <class className="org.intermine.model.bio.DrugType">
    <fields>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.GeneRIF">
    <fields>
      <fieldconfig fieldExpr="phrase"/>
      <fieldconfig fieldExpr="sentence"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.InteractionExperiment">
    <fields>
      <fieldconfig fieldExpr="description"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinInteraction">
    <fields>
      <fieldconfig fieldExpr="representativePartner.primaryAccession"/>
      <fieldconfig fieldExpr="representativePartner.name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.ProteinDNAInteraction">
    <fields>
      <fieldconfig fieldExpr="role"/>
      <fieldconfig fieldExpr="interactionType"/>
      <fieldconfig fieldExpr="interactWith.ncbiGeneNumber"/>
      <fieldconfig fieldExpr="interactWith.symbol"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.CompoundGroup">
    <fields>
      <fieldconfig fieldExpr="identifier"/>
      <fieldconfig fieldExpr="name"/>
    </fields>
  </class>

  <class className="org.intermine.model.bio.StructuralDomainRegion">
    <fields>
      <fieldconfig fieldExpr="start"/>
      <fieldconfig fieldExpr="end"/>
      <fieldconfig fieldExpr="cathClassification.cathCode"/>
      <fieldconfig fieldExpr="cathClassification.description"/>
    </fields>
  </class>

<!-- exporters -->
  <tableExportConfig id="sequence" className="org.intermine.bio.web.export.SequenceHttpExporter"/>
  <tableExportConfig id="gff3" className="org.intermine.bio.web.export.GFF3HttpExporter"/>

<!-- reportdisplayers -->
  <reportdisplayers>

    <reportdisplayer javaClass="org.intermine.bio.web.displayer.CytoscapeNetworkDisplayer"
                     jspName="model/cytoscapeNetworkDisplayer.jsp"
                     replacesFields="interactions"
                     placement="Interactions"
                     types="Gene,Protein"/>

    <reportdisplayer javaClass="org.intermine.bio.web.displayer.GeneOntologyDisplayer"
                     jspName="model/geneOntologyDisplayer.jsp"
                     replacesFields="goAnnotation"
                     placement="Gene Ontology"
                     types="Gene,Protein"/>

    <reportdisplayer javaClass="org.intermine.bio.web.displayer.UniProtCommentsDisplayer"
                     jspName="model/uniProtCommentsDisplayer.jsp"
                     replacesFields="comments"
                     placement="Proteins"
                     types="Protein"/>

    <reportdisplayer javaClass="org.intermine.bio.web.displayer.ProteinDomainDisplayer"
                     jspName="model/proteinAnalysisResult.jsp"
                     replacesFields="proteinDomainRegions"
                     placement="Proteins"
                     types="Protein"/>
                     
    <reportdisplayer javaClass="org.intermine.bio.web.displayer.ProteinSequenceDisplayer"
                     jspName="model/proteinSequenceDisplayer.jsp"
                     replacesFields="sequence"
                     placement="summary"
                     types="Protein"/>

  </reportdisplayers>

  
<!-- widgets -->
<widgets>
      <enrichmentwidgetdisplayer id="pathway_enrichment" title="Pathway Enrichment"
                                 link="org.intermine.bio.web.widget.TmPathwayURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.PathwayDataLoader"
                                 label="Pathway"
                                 description="Pathways enriched for items in this list."
                                 filters="All datasets,KEGG,Reactome,NCI" 
                                 filterLabel="DataSet" 
                                 externalLink="multi: org.intermine.bio.web.widget.config.PathwayExternalLink" 
                                 typeClass="org.intermine.model.bio.Gene,org.intermine.model.bio.Protein"
                                 />

<!--
      <enrichmentwidgetdisplayer id="gene_set_enrichment" title="Gene Set Cluster Enrichment"
                                 link="org.intermine.bio.web.widget.GeneSetURLQuery"
                                 dataSetLoader="org.intermine.bio.web.widget.GeneSetDataLoader"
                                 label="Gene Set Cluster"
                                 description="Gene Set Cluster enriched for genes in this list."
                                 filters="Hierarchical,Heuristic" 
                                 filterLabel="DataSet" 
                                 typeClass="org.intermine.model.bio.Gene" />
