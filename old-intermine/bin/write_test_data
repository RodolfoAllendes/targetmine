#!/bin/bash

# The following environment variables need to be set
# INTERMINE: location of local InterMine installation

cd $INTERMINE

CP=$CLASSPATH

for i in $INTERMINE/lib/*.jar ; do
    CP=$CP:$i
done

CP=$CP:build/test:build/java:build/model/testmodel:build/model/fulldata

java -cp $CP org.intermine.objectstore.SetupDataTestCase $*

