<?xml version="1.0"?>
<model name="test" namespace="http://www.intermine.org/model/test#">
  <class name="org.intermine.model.test.LtdCompany" extends="org.intermine.model.test.Company" is-interface="true"/>
  <class name="org.intermine.model.test.Charity" extends="org.intermine.model.test.Organisation" is-interface="true"/>
  <class name="org.intermine.model.test.OrganisationType" is-interface="true">
    <attribute name="type" type="java.lang.String"/>
    <reference name="companyModel" referenced-type="org.intermine.model.test.CompanyModel"/>
  </class>
  <class name="org.intermine.model.test.Company" extends="org.intermine.model.test.Organisation" is-interface="true">
    <attribute name="vatNumber" type="java.lang.Integer"/>
    <reference name="address" referenced-type="org.intermine.model.test.Address"/>
    <collection name="departments" referenced-type="org.intermine.model.test.Department" ordered="true" reverse-reference="company"/>
  </class>
 <class name="org.intermine.model.test.BigBusiness" extends="org.intermine.model.test.Company" is-interface="true"/>
  <class name="org.intermine.model.test.Department" is-interface="true">
    <reference name="company" referenced-type="org.intermine.model.test.Company" reverse-reference="departments"/>
  </class>
  <class name="org.intermine.model.test.CompanyModel" is-interface="true">
    <attribute name="model" type="java.lang.String"/>
  </class>
  <class name="org.intermine.model.test.Address" is-interface="true">
    <attribute name="address" type="java.lang.String"/>
  </class>
  <class name="org.intermine.model.test.Organisation" is-interface="true">
    <attribute name="profitable" type="java.lang.Boolean"/>
    <attribute name="name" type="java.lang.String"/>
    <reference name="organisationType" referenced-type="org.intermine.model.test.OrganisationType"/>
  </class>
</model>
