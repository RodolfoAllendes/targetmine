<?xml version="1.0"?>

<classes>
	<class name="Gene" is-interface="true">
		<attribute name="primaryIdentifier" type="java.lang.String"/>
		<collection name="reactions" referenced-type="Reaction" reverse-reference="gene1"/>
	</class>
	<class name="Reaction" is-interface="true">
    	<attribute name="name" type="java.lang.String"/>
		<reference name="gene1" referenced-type="Gene" reverse-reference="reactions"/>
		<reference name="gene2" referenced-type="Gene" />
    	<attribute name="text" type="java.lang.String"/>
    	<collection name="types" referenced-type="ReactionType"/>
    	<collection name="modifications" referenced-type="Modification" reverse-reference="reactions"/>
        <collection name="dataSets" referenced-type="DataSet"/>
	</class>
	<class name="ReactionType" is-interface="true">
    	<attribute name="name" type="java.lang.String"/>
	</class>
	<class name="Modification" is-interface="true">
		<attribute name="type" type="java.lang.String" />
		<attribute name="position" type="java.lang.Integer" />
		<attribute name="residue" type="java.lang.String" />
		<collection name="reactions" referenced-type="Reaction" reverse-reference="modifications"/>
		<reference name="protein" referenced-type="Protein" reverse-reference="modifications"/>
		<collection name="dataSets" referenced-type="DataSet"/>
	</class>
</classes>
