<?xml version="1.0"?>

<classes>
	<class name="Gene" is-interface="true">
		<attribute name="primaryIdentifier" type="java.lang.String"/>
		<collection name="reactions" referenced-type="Reaction" reverse-reference="gene1"/>
	</class>

	<class name="Reaction" is-interface="true">
    	<attribute name="name" type="java.lang.String"/>
		<reference name="gene1" referenced-type="Gene" reverse-reference="reactions" />
		<reference name="gene2" referenced-type="Gene" />
    	<collection name="types" referenced-type="ReactionType" reverse-reference="reaction"/>
	</class>
	<class name="ReactionType" is-interface="true">
    	<attribute name="name" type="java.lang.String"/>
    	<reference name="reaction" referenced-type="Reaction" reverse-reference="types" />
		<collection name="pathways" referenced-type="Pathway" />
	</class>

</classes>
