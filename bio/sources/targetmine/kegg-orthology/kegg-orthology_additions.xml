<?xml version="1.0"?>
<classes>
	
	<class name="GeneHomolog" is-interface="true">
		<attribute name="koNumber" type="java.lang.String"/>
		<attribute name="type" type="java.lang.String"/>
		<reference name="source" referenced-type="Gene" reverse-reference="homologs"/>
