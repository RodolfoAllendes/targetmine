<?xml version="1.0"?>
<!-- This file details any extensions needed to the data model to store data from this source, everything else is automatically generated from the model description so this is all we need to do to add to the model. -->
<classes>
   <class name="ProteinStructure" is-interface="true">
        <collection name="hetGroups" referenced-type="HetGroup" reverse-reference="structures"/>
   </class>
   <class name="HetGroup" extends="Compound" is-interface="true">
     <attribute name="hetId" type="java.lang.String"/>
     <collection name="structures" referenced-type="ProteinStructure" reverse-reference="hetGroups"/>
     <!-- To be extended -->
   </class>

    <class name="Compound" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="name" type="java.lang.String"/>
		<reference name="compoundGroup" referenced-type="CompoundGroup" reverse-reference="compounds"/>
    </class>

    <class name="CompoundGroup" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="name" type="java.lang.String"/>
		<collection name="compounds" referenced-type="Compound" reverse-reference="compoundGroup"/>
		<collection name="proteinInteractions" referenced-type="ProteinCompoundGroupInteraction" reverse-reference="compoundGroup" />
    </class>

	<!-- For post processing -->	
	<class name="ProteinCompoundGroupInteraction" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<reference name="compoundGroup" referenced-type="CompoundGroup" reverse-reference="proteinInteractions"/>
		<reference name="protein" referenced-type="Protein" reverse-reference="compoundGroupInteractions"/>
		<reference name="dataSet" referenced-type="DataSet"/>
    </class>

	<class name="Protein" is-interface="true">
		<collection name="compoundGroupInteractions" referenced-type="ProteinCompoundGroupInteraction" reverse-reference="protein"/>
    </class>

</classes>