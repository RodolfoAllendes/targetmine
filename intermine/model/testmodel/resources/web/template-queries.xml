<query-list>
  <query name="employeesFromCompanyAndDepartment" model="testmodel" view="Employee">
    <node path="Employee.department.company.name">
      <constraint op="=" value="CompanyA"/>
    </node>
    <node path="Employee.department.name">
      <constraint op="LIKE" value="Department%"/>
    </node>
  </query>
  <query name="employeesOfACertainAge" model="testmodel" view="Employee">
    <node path="Employee.age">
      <constraint op=">" value="35"/>
    </node>
  </query>
  <query name="employeeByName" model="testmodel" view="Employee">
    <node path="Employee.name">
      <constraint op="=" value=""/>
    </node>
  </query>
  
</query-list>
